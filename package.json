{
  "name": "my-api",
  "version": "1.0.0",
  "description": "My node api",
  "main": "index.ts",
  "repository": "git@github.com:murilolmartins/my-api.git",
  "author": "Murilo Martins <murilolmartins1@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "dotenv -e .env.development -- tsx watch src/infra/server.ts ",
    "lint": "npx eslint src --ext .ts --fix --max-warnings=0",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "dotenv -e .env.test -- jest -i",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test:db": "yarn docker:up && yarn migrate:test && yarn test",
    "test:unit": "jest --config ./jest/config/unit.config.js --coverage",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "ncu": "ncu -i --dep prod,dev --format group -p yarn"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "fastify": "^4.19.0",
    "fastify-plugin": "^4.5.0",
    "fastify-type-provider-zod": "^1.1.9",
    "fastify-zod": "^1.3.2",
    "http-status-codes": "^2.2.0",
    "pino-pretty": "^10.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-mock-extended": "2.0.4",
    "npm-check-updates": "^16.10.13",
    "prettier": "^2.8.8",
    "timekeeper": "^2.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "*"
  }
}
